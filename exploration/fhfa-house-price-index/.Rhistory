expand <- function(obj, sml, big){#
  if (big %% sml != 0) #
    stop("longer object length is not a multiple of shorter object length", call. = F)#
  seq <- rep(seq_len(sml), length = big)#
    #
  if (length(dim(obj)) == 2) {#
    obj[seq, , drop = FALSE]#
  } else {#
    obj[seq]#
  }#
    #
}#
